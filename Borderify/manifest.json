{
	"description":"Adds a solid red border to all webpages matching mozilla.org",
	"manifest_version": 2,
	"name": "Borderify",
	"version":"1.0",
	"icons":{
		"48":"icons/png1.png"
			},

	"content_scripts":
	[
		{
			"matches":["*://*.mozilla.org/*"],
			"js":["borderify.js"]
		}
	]
}


