{
	"description": "Adds a solid red border to all webpage matching mozilla.org.",
	"manifest_version":1,
	"name":"Borderify",
	"version":"1.0",
	"homepage_url":"https://github.com/mdn/webextension-example/tree/master/borderify",
	"icons":
	{
		"48":"icons/icon.png"
	},
	"content_scripts":
	[
		{
		"matches":["://*/.mozilla.org/*"],
		"js":["borderify.js"]
		}
	]
}