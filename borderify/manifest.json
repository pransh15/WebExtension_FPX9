
{
	"description" : "adds a solid red border to all webpages matching mozilla.org.",
	"manifest_version": 2,
	"name" : "Borderify",
	"version" : "1.0",
	"homepage_url" : "https:/github.com/mdn/webextensions-examples/tree/master/borderify",
	"icons" : {"48" : "icons/123.png"},

"content_scripts":[
  {
	"matches" : ["*://*.mozilla.org/*"],
	"js": ["borderify.js"]
  }
 ]
}
