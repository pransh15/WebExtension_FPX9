{
	"description":"adds a solid red border to all webpage matching:mozilla.org.",
	"manifest_version":2,
	"name":"borderify",
	"version":"1.0",
	"homepage_url": "http://github.com/mdn/webextensions-example/tree/master/borderify",
	"icons":{
		"48":"icons/i.png"
	},
	"content_scripts":
	[
		{
		"matches":["*://*.mozilla.org/*"],
		"js":["borderify.js"]
		}
	]
}